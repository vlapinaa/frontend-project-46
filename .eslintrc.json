{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "plugins": [
        "import"
    ],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn"
    }
}