{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "plugins": [
        "import"
    ],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "import/extensions": "off",
        "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }]
    }
}